<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fh.swei.SecondaryController">
   <children>
      <VBox alignment="CENTER" prefHeight="397.0" prefWidth="635.0" spacing="20.0">
          <children>
            <ListView fx:id="photogrList" prefHeight="200.0" prefWidth="108.0" />
            <Button fx:id="choosephot" mnemonicParsing="false" onAction="#choosePhot" text="Choose the selected photographer" />
            <Button fx:id="addphot" mnemonicParsing="false" onAction="#addphotogr" text="Add a new photographer" />
              <Button fx:id="closeButton" onAction="#switchToPrimary" text="Back" />
          </children>
      
          <padding>
              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
          </padding>
      </VBox>
   </children>
</HBox>
